<script asp-append-version="true" src="~/js/star-rating.js"></script>
<script asp-append-version="true">
    $(document).ready(function () {
        $('.rating-star').ffrating({ isStar: true, readonly: true, showSelectedRating: true, min: 1, max: 5 });
        $('.add-star').ffrating({ isStar: true, readonly: false, showSelectedRating: true, min: 1, max: 5 });

    });
</script>
<script asp-append-version="true" src="~/js/lavazi.jquery.js" type='text/javascript'></script>
<script asp-append-version="true" type="text/javascript">
    $(document).ready(function () {
        $('.menu_main>ul').lavazi({ background: "rgba(255,255,255,.2)", transitionTime: 500, theme: 'simple', height: 60, mode: 'belowBar', opacity: .3 });
    });
</script>
<script asp-append-version="true" src="~/js/jquery.slicknav.js"></script>
<script asp-append-version="true" type="text/javascript">
    $(document).ready(function () {
        $('#menu').slicknav();
    });
</script>


